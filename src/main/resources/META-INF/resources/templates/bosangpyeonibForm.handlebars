<div id="dvBosangPyeonibInfoWin" class="mapInnerBox type2"> 
    <div class="mibHeader"><span class="titIcon {{#xif " kind == '보상' "}} bosang {{else}} pyeonib {{/xif}}">{{kind}}</span></div> 
    <div class="mibBody" style="height:360px;">
       	<!-- list -->
    	<div id="dvBosangPyeonibList" class="listType" style="display:none;"></div>

		<!-- table -->
		<div id="dvBosangPyeonibTable" class="tableType" style="display:none;">
			<table class="tableStyle borderStyle left">
				<tbody>
				<tr>
					<th>물건 소재지</th>
					<td><span id="bpMulgeonsojaeji">{{mulgeonsojaeji}}</span></td>
				</tr>
				<tr>
					<th>공고기관</th>
					<td><span id="bpGonggogigwan">{{gonggogigwan}}</span></td>
				</tr>
				<tr>
					<th>공고일</th>
					<td><span id="bpGonggoil">{{gonggoil}}</span></td>
				</tr>
				<tr>
					<th>사업명</th>
					<td><span id="bpSaeobname">{{saeobname}}</span></td>
				</tr>
				<tr>
					<th>공고번호</th>
					<td><span id="bpGonggobeonho">{{gonggobeonho}}</span></td>
				</tr>
				<tr>
					<th>시설종류</th>
					<td><span id="bpSiseolkind">{{siseolkind}}</span></td>
				</tr>
				<tr>
					<th>사업 시행자</th>
					<td><span id="bpSaeobsihaengja">{{saeobsihaengja}}</span></td>
				</tr>
				</tbody>
			</table>
		</div>       
    </div>
    
    <div id="dvMib" class="mibBody" style="border-radius: 0 0 0 0;">
    	<div class="munuType" data-address="" data-pnu="" data-lng="" data-lat="">
        	<span class="link" style="border-bottom-style:none; padding-left: 20px;"><button type="button" class="mibIcon_01" style="border-right-style:none; font-size:14px;" onclick="hotplace.spot.selectCategory(this)" data-category="SUJI_BOONSEOK">수지분석</button></span>
			<span class="link" style="border-bottom-style:none; padding-right: 20px;"><button type="button" class="mibIcon_02" style="border-right-style:none; font-size:14px;" onclick="hotplace.spot.selectCategory(this, '{{kindCode}}')" data-category="GWANSIM_MULGEON" data-extra="">관심물건 등록</button></span>
		</div>
	</div>
    
    <div class="mibfooter">
    	<button type="button" id="btnBosangPyeonibPano" class="positionView" data-name="위치보기">위치보기</button>
    	<button type="button" id="btnBosangPyeonibList" class="goList" style="displat:none;">목록으로</button>
        <button type="button" id="btnBosangPyeonibThumbClose" class="close">닫기</button>
    </div>
</div>