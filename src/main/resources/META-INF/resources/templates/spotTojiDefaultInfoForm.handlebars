<div class="modal-dialog" role="document">
	<div class="modal-content">
		<div class="modal-header">
			<h2 class="modal-title">
				토지기본정보
				<span class="sText">( {{address}} )</span>
			</h2>
			<button type="button" class="close" data-dismiss="modal" aria-label="Close">
				<i class="ambicon-015_mark_times"></i><span class="hidden">닫기</span>
			</button>
		</div>
		<div class="modal-body">

			<div>
			
				<!-- ----------------------- -->
				<div class="rowBox">

					<!-- 좌측 -->
					<div class="g_column w_2_1">
						
						<!-- 기본정보 -->
						<div class="unit">							
							<div class="unit_tit">
								<span class="sTit color_black noBullt">기본정보</span>
							</div>
							<div class="unit_cont bgWhite">
								<table class="tableStyle borderStyle topBold left">
									<colgroup>
										<col style="width:25%;">
										<col style="width:75%;">
									</colgroup>
									<tbody>
										<tr>
											<th>소재지(지번)</th>
											<td>{{address}}</td>
										</tr>
										<tr>
											<th>지목</th>
											<td>{{jimok}}</td>
										</tr>
										<tr>
											<th>면적(㎡)</th>
											<td>{{area}}</td>
										</tr>
										<tr>
											<th>공시지가</th>
											<td>{{gongsi}}</td>
										</tr>
										<tr>
											<th>공시지가 기준년월</th>
											<td>{{gongsiYm}}</td>
										</tr>
									</tbody>
								</table>
							</div>
						</div>
						<!-- //기본정보 -->
					</div>

					<!-- //좌측 -->

					<!-- 우측 -->
					<div class="g_column w_2_1">
						<!-- Luris 도면 -->
						<div class="unit">							
							<div class="unit_tit">
								<span class="sTit color_black noBullt">도면</span>
							</div>
							<div class="unit_cont bgWhite">
								<table class="tableStyle borderStyle topBold left">
									<colgroup>
										<col style="">
									</colgroup>
									<tbody>
										<tr>
											<td>
												{{#if image}} 						
												<a href="#"><img id="sujiLurisDrawing" src="{{image}}"></a>
												{{else}}
												<img src="data:image/png;base64,">
												{{/if}}											
											</td>
										</tr>
									</tbody>
								</table>
							</div>
						</div>
						<!-- //Luris 도면 -->
					</div>
					<!-- //우측 -->

				</div>
            </div>
        </div>
    </div>
</div>